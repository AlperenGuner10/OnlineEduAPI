@model CreateContactDto
@{
	ViewData["Title"] = "İletişim Bilgisi Ekleme Sayfası";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
	<label>Harita URL</label>
	<input type="text" asp-for="MapUrl" class="form-control">
	<br>
	<label>Adres</label>
	<input type="text" asp-for="Address" class="form-control">
	<br>
	<label>Telefon</label>
	<input type="text" asp-for="Phone" class="form-control">
	<br>
	<label>E-Mail</label>
	<input type="text" asp-for="Email" class="form-control">
	<br>
	<button class="btn btn-outline-primary">Ekle</button>
</form>

